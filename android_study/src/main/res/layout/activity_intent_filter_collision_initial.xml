<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".intents.intentfiltercollision.IntentFilterCollisionInitialActivity"
    android:gravity="center"
    android:orientation="vertical">

    <TextView
        android:id="@+id/run_activities_same_intents_text_view"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/same_intents_text_view_text" />

    <Button
        android:id="@+id/run_activities_same_intents_button"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/same_intents_button_text" />

</LinearLayout>
